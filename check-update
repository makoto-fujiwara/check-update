    #  (7) ------------------------------------------
# delimiter is perid(.)  rtf2LaTeX.1.5
#   if ($PKGBASE .'.'. $PKGVERSION =~ /$DISTBASE$/i ) {
    if ($DISTBASE  =~ /^$PKGBASE.$PKGVERSION$/i ) { # ------- GetDistBase (7)
