    } elsif ( $available eq '0.0' || $available eq '') {
	$console_color = 'magenta';
	if    ($MasterSiteStat == 404 )	{ $NotFound++; }
	elsif ($MasterSiteStat == 403 )	{ $Forbidden++;}
	elsif ($MasterSiteStat == 550 )	{ $Forbidden++;}
