    # (distname) ast-ksh-2012-08-01 -> (pkgname) ast-ksh-20120801
        $thisreturn = GetDistBaseReduce($PKGBASE, $PKGNAME, $PKGVERSION, $DISTBASE);
    if ($thisreturn) { return $thisreturn; }
