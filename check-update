	$SpecialPatternY = ' m|.*' . $pkgname. '-([0-9.]+-p[0-9]+)\.tar\..*|; $version = $1; $version =~ s/-p/./; ' ;
  print STDERR sprintf("%4d <", __LINE__ ),  (caller 0)[3], '>:', (caller 0)[2],  ' ------------------',"\n",
