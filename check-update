    $VERSION_CANDIDATE =~ s/$PKGBASE-//;
    $VERSION_CANDIDATE =  MonthToDigit($VERSION_CANDIDATE);	# GetDistBase (10) ----------
print STDERR sprintf("%4d ", __LINE__ ), '(PKGBODY) ', $PKGBODY,' (CAND) ', $VERSION_CANDIDATE,"\n" if $DEBUG_DISTBASE;
    my($testA) = $PKGBASE;
    my($testB) = $DISTBASE;
